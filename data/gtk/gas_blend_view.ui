<!-- Created with Cambalache 0.96.1 -->
<interface>
  <!-- interface-name gas_blend_view.ui -->
  <requires lib="gtk" version="4.16"/>
  <template class="GasBlendView" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkBox">
        <property name="hexpand">True</property>
        <child>
          <object class="GtkLabel">
            <property name="halign">start</property>
            <property name="hexpand">True</property>
            <property name="label">Available gases</property>
          </object>
        </child>
        <child>
          <object class="GtkButton">
            <property name="icon-name">list-add-symbolic</property>
            <signal name="clicked" handler="add_gas"/>
            <style>
              <class name="flat"/>
            </style>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GasBlendColumnView" id="gas_blend_column_view"/>
    </child>
  </template>
</interface>
